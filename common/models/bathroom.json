{
  "name": "Bathroom",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "rating": {
      "type": "number"
    },
    "type": {
      "type": "string"
    },
    "latitude": {
      "type": "number"
    },
    "longitude": {
      "type": "number"
    },
    "address": {
      "type": "string"
    },
    "start_time": {
      "type": "date"
    },
    "end_time": {
      "type": "date"
    },
    "maintenance_start": {
      "type": "date"
    },
    "maintenance_end": {
      "type": "date"
    },
    "maintenance_days": {
      "type": "string"
    },
    "bookmarked": {
      "type": "boolean"
    },
    "amenities": {
      "type": [
        "string"
      ]
    },
    "descriptions": {
      "type": [
        "string"
      ]
    },
    "image_url": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "submittedReviews": {
      "type": "hasMany",
      "model": "Account",
      "through": "Review",
      "foreignkey": "bathroomId"
    },
    "bookmarker": {
      "type": "hasAndBelongsToMany",
      "model": "Account",
      "foreignKey": ""
    },
    "submittedBy": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": "accountId"
    }
  },
  "acls": [],
  "methods": {}
}
